{
    "name": "fjw-go-extend",
    "displayName": "fjw-go-extend",
    "description": "",
    "version": "0.0.1",
    "publisher": "go-define",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.myTestCMD",
        "onCommand:extension.expandErrorReturn",
        "onCommand:extension.foldErrorReturn"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "extension.myTestCMD",
                "title": "Test"
            },
            {
                "command": "extension.expandErrorReturn",
                "title": "Expand",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.foldErrorReturn",
                "title": "Fold"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
